{
  "data": {"url": "../latencies.json"},
  "layers": [
    {
      "mark": "bar",
      "config": {
        "mark": {"color": "#999"}
      },
      "encoding": {
        "x": {
          "field": "name",
          "type": "ordinal",
          "axis": {"title": null}
        },
        "y": {
          "field": "latency.value",
          "type": "quantitative",
          "axis": {"title": "frame latency (ms)"}
        }
      }
    },

    {
      "mark": "rule",
      "config": {"mark": {"color": "#000"}},
      "encoding": {
        "x": {"field": "name", "type": "ordinal"},
        "y": {"field": "latency.err_min", "type": "quantitative"},
        "y2": {"field": "latency.err_max", "type": "quantitative"}
      }
    },
    {
      "mark": "tick",
      "config": {"mark": {"color": "#000"}},
      "encoding": {
        "x": {"field": "name", "type": "ordinal"},
        "y": {"field": "latency.err_min", "type": "quantitative"},
        "size": {"value": 5}
      }
    },
    {
      "mark": "tick",
      "config": {"mark": {"color": "#000"}},
      "encoding": {
        "x": {"field": "name", "type": "ordinal"},
        "y": {"field": "latency.err_max", "type": "quantitative"},
        "size": {"value": 5}
      }
    }
  ]
}
