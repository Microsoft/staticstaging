{
  "data": {"url": "../latencies.json"},
  "layers": [
    {
      "mark": "bar",
      "config": {
        "mark": {"color": "#999"}
      },
      "encoding": {
        "x": {
          "field": "name",
          "type": "ordinal",
          "axis": {"title": null}
        },
        "y": {
          "field": "value",
          "type": "quantitative",
          "axis": {"title": "frame latency (ms)"}
        }
      }
    },

    {
      "mark": "rule",
      "encoding": {
        "x": {"field": "name", "type": "ordinal"},
        "y": {
          "field": "err_min",
          "type": "quantitative"
        },
        "y2": {
          "field": "err_max",
          "type": "quantitative"
        }
      }
    }
  ]
}
